{
    "name": "aerni/statamic-livewire-forms",
    "description": "Use Statamic forms with Laravel Livewire",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "statamic/cms": "^3.0",
        "jonassiewertsen/statamic-livewire": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Aerni\\StatamicLivewireForms\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Livewire Forms",
            "description": "Statamic Livewire Forms"
        },
        "laravel": {
            "providers": [
                "Aerni\\StatamicLivewireForms\\ServiceProvider"
            ]
        }
    }
}
